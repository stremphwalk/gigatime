# Database
DATABASE_URL=your_database_url_here

# Session
SESSION_SECRET=your_session_secret_here

# Auth0 Configuration (Server)
AUTH0_SECRET=your_auth0_secret_here
AUTH0_BASE_URL=http://localhost:5001
AUTH0_CLIENT_ID=your_auth0_client_id_here
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here
AUTH0_ISSUER_BASE_URL=https://your-domain.auth0.com
AUTH0_AUDIENCE=your_auth0_api_audience_here

# Auth0 Configuration (Client - Vite)
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_auth0_client_id_here
VITE_AUTH0_AUDIENCE=your_auth0_api_audience_here

# Soniox (for dictation)
SONIOX_API_KEY=your_soniox_api_key_here

# Amazon Nova Micro (AI)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# Environment
NODE_ENV=development
